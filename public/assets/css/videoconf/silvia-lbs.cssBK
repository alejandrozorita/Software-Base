        .sprite {
            display: inline-block;
            overflow: hidden;
            background-repeat: no-repeat;
            background-image: url('assets/img/drawpad.png');
            float:left;
            margin-right:10px;
        }
        .draw {
            width: 42px;
            height: 43px;
            background-position: -222px -2px
        }
        .line {
            width: 42px;
            height: 43px;
            background-position: -178px -2px
        }
        .trash {
            width: 42px;
            height: 43px;
            background-position: -46px -2px
        }
        .erase {
            width: 42px;
            height: 43px;
            background-position: -2px -2px
        }
        .toolbar {
            #position: absolute;
            padding: 0;
            margin: 0;
            z-index : 4;
        }
        .toolbar li {
            display: block;
            list-style: none;
            margin-bottom: 10px;
            border: 2px solid #000;
            background-color: #d9dadb;
            cursor: pointer;
        }

        .toolbar li.selected,.toolbar li.hover {
            border: 4px solid #000;
        }

        .sharedCanvas, .myCanvas {
            border: 2px solid #ccc;
