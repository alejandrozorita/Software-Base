/*chat*/

.shout_box {
    background: #627bae none repeat scroll 0 0;
    bottom: 0;
    overflow: hidden;
    position: fixed;
    right: 1%;
    width: 260px;
    z-index: 9;
}
.shout_box .chat .fa.fa-times {
    background: rgba(0, 0, 0, 0) url("images/close_btn.png") no-repeat scroll 0 0;
    float: right;
    height: 15px;
    width: 15px;
}
.shout_box .chat .fa.fa-times:hover {
    background: rgba(0, 0, 0, 0) url("images/close_btn.png") no-repeat scroll 0 -16px;
}
.shout_box .chat .open_btn {
    background: rgba(0, 0, 0, 0) url("images/close_btn.png") no-repeat scroll 0 -32px;
    float: right;
    height: 15px;
    width: 15px;
}
.shout_box .chat .open_btn:hover {
    background: rgba(0, 0, 0, 0) url("images/close_btn.png") no-repeat scroll 0 -48px;
}
.shout_box .chat {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    border-color: rgba(0, 39, 121, 0.76) rgba(0, 39, 121, 0.76) -moz-use-text-color;
    border-image: none;
    border-style: solid solid none;
    border-width: 1px 1px medium;
    color: #fff;
    cursor: pointer;
    font: bold 11px "lucida grande",tahoma,verdana,arial,sans-serif;
    padding: 5px 3px 5px 5px;
}
.shout_box .chat:hover {
    background-color: #627bae;
}
.shout_box .message_box {
    background: #ffffff none repeat scroll 0 0;
    border: 1px solid #ccc;
    height: 200px;
    overflow: auto;
}
.shout_msg {
    background-color: #e5e9ec;
    border-bottom: 1px solid #f3f3f3;
    border-radius: 7px;
    color: #7c7c7c;
    display: block;
    float: left;
    font: 11px "lucida grande",tahoma,verdana,arial,sans-serif;
    margin-bottom: 10px;
    margin-left: 2%;
    margin-top: 2%;
    padding: 0 5px 5px;
    width: 55%;
}
.message_box:last-child {
    border-bottom: medium none;
}
time {
    color: #d5d5d5;
    float: right;
    font: 11px "lucida grande",tahoma,verdana,arial,sans-serif;
}
.shout_msg .username {
    margin-bottom: 10px;
    margin-top: 10px;
}
.user_info input {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    border-color: -moz-use-text-color #ccc #ccc;
    border-image: none;
    border-style: none solid solid;
    border-width: medium 1px 1px;
    font: 11px "lucida grande",tahoma,verdana,arial,sans-serif;
    height: 25px;
    padding: 3px 0 0 3px;
    width: 100%;
}
.shout_msg .username {
    display: block;
    font-weight: bold;
}

.close_btn {
    float: right;
    width: 38%;
}


.shout_izq {
    background-color: #0ba195;
    border-radius: 7px;
    color: #fff;
    display: block;
    float: right;
    margin-bottom: 10px;
    margin-right: 3%;
    padding: 0 5px 5px;
    width: 50%;
}
.message {
    float: left;
    white-space: normal;
    width: 88%;
    word-wrap: break-word;
}


